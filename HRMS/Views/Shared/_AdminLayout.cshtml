    <script src="~/Adminlayout/js/jquery.sparkline.min.js" type="text/javascript"></script>
    <script src="~/Adminlayout/js/jquery.textareaCounter.plugin.js"></script>
    <!-- END PAGE LEVEL PLUGINS -->
    <!-- BEGIN PAGE LEVEL SCRIPTS -->
    <script src="~/Adminlayout/js/metronic.js" type="text/javascript"></script>
    <script src="~/Adminlayout/js/layout.js" type="text/javascript"></script>
    <script src="~/Adminlayout/js/quick-sidebar.js" type="text/javascript"></script>
    <script src="~/Adminlayout/js/demo.js" type="text/javascript"></script>
    <script src="~/Adminlayout/js/index.js" type="text/javascript"></script>
    <script src="~/Adminlayout/js/tasks.js" type="text/javascript"></script>
    <!-- END PAGE LEVEL SCRIPTS -->
    <script>
        jQuery(document).ready(function () {
            //Metronic.init(); // init metronic core componets
            Layout.init(); // init layout
