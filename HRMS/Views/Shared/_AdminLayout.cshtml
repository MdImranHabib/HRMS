    <script src="~/Adminlayout/plugins/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/Adminlayout/js/bootstrap-hover-dropdown.min.js" type="text/javascript"></script>
    <script src="~/Adminlayout/plugins/jquery-slimscroll/jquery.slimscroll.min.js" type="text/javascript"></script>
    <script src="~/Adminlayout/js/jquery.blockui.min.js" type="text/javascript"></script>
    <script src="~/Adminlayout/js/jquery.cokie.min.js" type="text/javascript"></script>
    <script src="~/Adminlayout/plugins/uniform/jquery.uniform.min.js" type="text/javascript"></script>
